package com.assisted.practice;


public class x {
		public void methodx() {
			System.out.println("Base class is A");
		}
	}
	
	class y extends x{
		public void methody() 
		{
		System.out.println("child class method is B");
		}
		}
		class z extends y{
			public void methodz() 
			{
			System.out.println("child class method is B");
			}
		public static void main(String[] args) 
		{
		
				z obj=  new z();
				obj.methodx();
				obj.methody();
				obj.methodz();
		}
}
	
	
	
	
	
