package com.assisted.practice;

public class inheritance {
	class A{
		public void methodA() {
			System.out.println("Base class is A");
		}
	}
	
	class B extends A{
		public void methodB() 
		{
		System.out.println("child class method is B");
		}
		public static void main(String args[]) 
		{
		
			     B obj=  new B();
				obj.methodA();
				obj.methodB();
		}
}
	
	
}
