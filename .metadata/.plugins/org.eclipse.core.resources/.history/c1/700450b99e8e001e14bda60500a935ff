package com.assisted.practice;

	
	public class class_constructor_object {
		  int modelYear;
		  String modelName;

		  public class_constructor_object(int year, String name) {
		    modelYear = year;
		    modelName = name;
		  }

		  public static void main(String[] args) {
			  class_constructor_object myCar = new class_constructor_object(1969, "Mustang");
		    System.out.println(myCar.modelYear + " " + myCar.modelName);
		  }
		}



