package com.assisted.practice;

	// understanding the class, contructor and objects
	public class class_constructor_object {  // creating a class
		  int modelYear;
		  String modelName;

		  public class_constructor_object(int year, String name) {  //constructor
		    modelYear = year;
		    modelName = name;
		  }

		  public static void main(String[] args) {
			  class_constructor_object myCar = new class_constructor_object(1969, "Mustang");  // creating an object 
		    System.out.println(myCar.modelYear + " " + myCar.modelName);
		  }
		}



